FUNCTION V3_YANG : REAL
VAR_INPUT
    A : Vector_3; // Input variable of derived type Vector_3
END_VAR

VAR
    la : REAL; // Local variable of type REAL
END_VAR
la := V3_ABS(a);
IF la > 0.0 THEN
	V3_YANG := ACOS(A.Y / la);
END_IF;

